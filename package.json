{
  "name": "zhihuliteweb",
  "private": true,
  "version": "0.0.0",
  "description": "Zhihu Lite Web App built with Framework7 Vue",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npx workbox generateSW workbox-config.js",
    "preview": "vite preview",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "framework7": "^9.0.2",
    "framework7-icons": "^5.0.5",
    "framework7-vue": "^9.0.2",
    "material-icons": "^1.13.14",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "vite": "^7.3.0",
    "cpy-cli": "^6.0.0",
    "workbox-cli": "^7.4.0"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ]
}